{
  "expo": {
    "name": "Notic",
    "slug": "notic-mobile",
    "description": "Mobile cross-platform app created with React-Native for a manage notes app.",
    "privacy": "public",
    "version": "0.0.1",
    "platforms": [
      "ios",
      "android"
    ],
    "orientation": "portrait",
    "icon": "./assets/custom/icon-01.png",
    "splash": {
      "image": "./assets/custom/splash-01.png",
      "resizeMode": "cover",
      "backgroundColor": "#AAEDEE"
    },
    "updates": {
      "fallbackToCacheTimeout": 1500
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/custom/icon-01.png",
        "backgroundColor": "#AAEDEE"
      },
      "package": "app.netlify.notic"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}